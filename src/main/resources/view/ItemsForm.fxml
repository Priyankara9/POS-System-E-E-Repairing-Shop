<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXTextField?>
<?import com.jfoenix.controls.JFXTreeTableView?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #d1e4ee;"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controller.ItemsFormController">
   <children>
      <GridPane layoutY="14.0" prefHeight="500.0" prefWidth="800.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="261.0" minWidth="0.0" prefWidth="58.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="680.0" minWidth="10.0" prefWidth="660.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="176.0" minWidth="10.0" prefWidth="68.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="31.0" minHeight="22.0" prefHeight="31.0" />
            <RowConstraints maxHeight="257.0" minHeight="10.0" prefHeight="14.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="257.0" minHeight="10.0" prefHeight="107.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="257.0" minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="372.0" minHeight="10.0" prefHeight="286.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="161.0" minHeight="8.0" prefHeight="36.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <GridPane GridPane.columnIndex="1" GridPane.rowIndex="2">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="118.0" minWidth="10.0" prefWidth="103.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="126.0" minWidth="0.0" prefWidth="40.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="266.0" minWidth="10.0" prefWidth="173.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="365.0" minWidth="10.0" prefWidth="40.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="317.0" minWidth="10.0" prefWidth="129.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="317.0" minWidth="10.0" prefWidth="40.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="284.0" minWidth="10.0" prefWidth="153.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <JFXTextField fx:id="txtCode" prefHeight="25.0" prefWidth="167.0" promptText="Item Code"
                                GridPane.halignment="LEFT" GridPane.valignment="CENTER" />
                  <JFXTextField fx:id="txtName" prefWidth="40.0" promptText="Name" GridPane.columnIndex="2"
                                GridPane.halignment="LEFT" GridPane.valignment="CENTER" />
                  <JFXTextField fx:id="txtCategory" prefWidth="40.0" promptText="Category" GridPane.columnIndex="4"
                                GridPane.halignment="LEFT" GridPane.valignment="CENTER" />
                  <JFXTextField fx:id="txtStatus" prefHeight="25.0" prefWidth="160.0" promptText="Status"
                                GridPane.columnIndex="6" GridPane.halignment="LEFT" GridPane.valignment="CENTER" />
                  <JFXTextField fx:id="searchCode" prefWidth="40.0" promptText="Item Code" GridPane.halignment="LEFT"
                                GridPane.rowIndex="1" GridPane.valignment="CENTER" />
                  <JFXButton onAction="#updateButtonOnAction" prefHeight="40.0" prefWidth="100.0"
                             style="-fx-background-color: #529add;" text="Update" textFill="#01060a"
                             GridPane.columnIndex="4" GridPane.rowIndex="1">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </JFXButton>
                  <JFXButton alignment="CENTER" onAction="#searchButtonOnAction" prefHeight="40.0"
                             prefWidth="102.0" style="-fx-background-color: #a8d00b;" text="Search"
                             textAlignment="CENTER" textFill="#171717" GridPane.columnIndex="2"
                             GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </JFXButton>
               </children>
            </GridPane>
            <JFXTreeTableView GridPane.columnIndex="1" GridPane.rowIndex="4">
               <columns>
                  <TreeTableColumn prefWidth="111.0" resizable="false" text="Item Code" />
                  <TreeTableColumn prefWidth="182.0" resizable="false" text="Product Name" />
                  <TreeTableColumn minWidth="0.0" prefWidth="129.0" resizable="false" text="Category" />
                  <TreeTableColumn prefWidth="121.0" resizable="false" text="Status" />
                  <TreeTableColumn prefWidth="119.0" resizable="false" text="Option" />
               </columns>
            </JFXTreeTableView>
            <Label alignment="CENTER" prefHeight="30.0" prefWidth="210.0" text="All Items " textAlignment="CENTER"
                   GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
               <font>
                  <Font name="Times New Roman Bold" size="24.0" />
               </font>
            </Label>
            <JFXButton onAction="#backButtonOnAction" textFill="#212324" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
