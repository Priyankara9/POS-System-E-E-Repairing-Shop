<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.CashierDashController">
   <children>
      <GridPane layoutX="10.0" layoutY="8.0" prefHeight="500.0" prefWidth="800.0"
                style="-fx-background-color: #eed1e5;">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="105.0" minWidth="10.0" prefWidth="78.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="160.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="160.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="200.0" minWidth="10.0" prefWidth="160.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="134.0" minWidth="10.0" prefWidth="56.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <JFXButton onAction="#customersButtonOnAction" prefHeight="65.0" prefWidth="130.0"
                       style="-fx-background-color: #289ec9;" text="Customers" textFill="#f4f7f8"
                       GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="1"
                       GridPane.valignment="CENTER">
               <font>
                  <Font name="Times New Roman Bold" size="12.0" />
               </font>
            </JFXButton>
            <JFXButton onAction="#userButtonOnAction" prefHeight="65.0" prefWidth="130.0"
                       style="-fx-background-color: #289ec9;" text="Users" textFill="#f4f7f8"
                       GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="1"
                       GridPane.valignment="CENTER">
               <font>
                  <Font name="Times New Roman Bold" size="12.0" />
               </font>
            </JFXButton>
            <JFXButton onAction="#ItemButtonOnAction" prefHeight="65.0" prefWidth="130.0"
                       style="-fx-background-color: #289ec9;" text="Items" textFill="#f4f7f8"
                       GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1"
                       GridPane.valignment="CENTER">
               <font>
                  <Font name="Times New Roman Bold" size="12.0" />
               </font>
            </JFXButton>
            <JFXButton onAction="#partsButtonOnAction" prefHeight="65.0" prefWidth="130.0"
                       style="-fx-background-color: #289ec9;" text="Spare Parts" textFill="#f4f7f8"
                       GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2"
                       GridPane.valignment="CENTER">
               <font>
                  <Font name="Times New Roman Bold" size="12.0" />
               </font>
            </JFXButton>
            <JFXButton onAction="#reportButtonOnAction" prefHeight="65.0" prefWidth="130.0"
                       style="-fx-background-color: #289ec9;" text="Reports" textFill="#f4f7f8"
                       GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="2"
                       GridPane.valignment="CENTER">
               <font>
                  <Font name="Times New Roman Bold" size="12.0" />
               </font>
            </JFXButton>
            <JFXButton onAction="#placeOrderButtonOnAction" prefHeight="65.0" prefWidth="130.0"
                       style="-fx-background-color: #a2f43d;" text="Place Order" textFill="#bf097f"
                       GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="3"
                       GridPane.valignment="CENTER">
               <font>
                  <Font name="Times New Roman Bold" size="12.0" />
               </font>
            </JFXButton>
            <JFXButton onAction="#statusButtonOnAction" prefHeight="65.0" prefWidth="130.0"
                       style="-fx-background-color: #289ec9;" text="Change Status" textFill="#fcf9f7"
                       GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.rowIndex="3"
                       GridPane.valignment="CENTER">
               <font>
                  <Font name="Times New Roman Bold" size="12.0" />
               </font>
            </JFXButton>
            <JFXButton onAction="#addPartsButtonOnAction" prefHeight="65.0" prefWidth="130.0"
                       style="-fx-background-color: #289ec9;" text="Add Spare Parts" textFill="#f4f7f8"
                       GridPane.columnIndex="3" GridPane.halignment="CENTER" GridPane.rowIndex="2"
                       GridPane.valignment="CENTER">
               <font>
                  <Font name="Times New Roman Bold" size="12.0" />
               </font>
            </JFXButton>
            <Label fx:id="lblTDateAndTime" text="Label" GridPane.columnIndex="3" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
